{
  "name": "qapass",
  "version": "0.0.1",
  "description": "QA Password Manager",
  "main": "src/index.ts",
  "scripts": {
    "start-server": "./node_modules/.bin/ts-node --files ./src/index.ts",
    "start": "./node_modules/.bin/nodemon -e ts,hbs -w ./ -x npm run start-server",
    "production": "node ./dist/index.js",
    "migration-run": "./node_modules/.bin/knex migrate:latest",
    "migration-revert": "./node_modules/.bin/knex migrate:rollback",
    "build-dist": "./node_modules/.bin/tsc",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test-watch": "NODE_ENV=test ./node_modules/.bin/jest --watch",
    "coverage": "./node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit --coverage"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/connect-flash": "0.0.35",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.38",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.16.0",
    "@types/express-handlebars": "^3.1.0",
    "@types/jest": "^25.1.3",
    "@types/jsonschema": "^1.1.1",
    "@types/knex": "^0.15.1",
    "@types/method-override": "0.0.31",
    "@types/mysql": "^2.15.8",
    "@types/node": "^10.12.18",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "jest": "^25.1.0",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^7.0.1",
    "typeorm-test-transactions": "^1.10.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.4.0",
    "csurf": "^1.11.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.1.0",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jsonschema": "^1.2.5",
    "knex": "^0.16.3",
    "method-override": "^3.0.0",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.14"
  }
}
